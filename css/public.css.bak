@charset 'utf-8';
	/*样式初始化*/
			div,body,ul,li,ol,p,form,input,h2{
				margin:0;
				padding:0;
			}
			body{
				font:12px/1.6 "Microsoft YaHei";
				color:#333;
				overflow-x:hidden;
			}	
			a{
			text-decoration:none;
			color:#666;
			}
			ul,ol{
			list-style:none;
			}
			img{
				margin:0;
				padding:0;
			}
	/*float清除*/
	.clearfix:after{
		content:'';
		display:block;
		clear:both;
	}
	.public-w{
		width:1190px;
	}
	
	
