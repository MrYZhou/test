@charset'utf-8';
.color-grey{
	background-color:#F5f5f5;
}
.content-rob{
	width:100%;
}
.content-rob .rob-wrap{
	margin:35px auto 10px;
}
.content-rob .rob-p > img{
	float:left;
	width:230px;
	height:294px;
	
}
.content-rob .rob-content{
	float:left;
	padding:0 20px;
	background-color:#FFF;
}
.content-rob .rob-content a{
	display:inline-block;
}
.content-rob .rob-content img{
	width:190px;
	height:190px;
	vertical-align:middle;
}
.content-rob .rob-content div{
	overflow:hidden;
	float:left;
	padding:30px 20px 0px 20px;
}

.content-rob .rob-content p > a{
	display:inline-block;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis;
	width:170px;
	color:#666;
	font-size:16px;
	line-height:20px;
	text-align:center;
	margin:12px 10px 2px;
} 
.content-rob .rob-content em{
	display:inline-block;
	float:left;
	font-size:22px;
	color:#ff4b6d;
}
.content-rob .rob-content del{
	/* position:relative;
	top:9px; */
	display:inline-block;
	float:right;
	margin-top:9px;
	font-size:14px;
	color:#999;
}

/*fastrob*/
.content-rob .fast-rob{
	width:100%;
}
.content-rob .fast-rob .fast-rob-wrap{
	height:150px;
	margin:0 auto;
}
.content-rob .fast-rob .fast-rob-wrap div{
	float:left;
}
.content-rob .fast-rob .fast-rob-wrap div.w-s{
	width:230px;
	height:150px;
	margin-left:10px;
}
.content-rob .fast-rob .fast-rob-wrap div a{
	display:block;
}
.content-rob .fast-rob .fast-rob-wrap  img{
	width:100%;
	height:150px;
}


