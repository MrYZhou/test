@charset'utf-8';
/*content-nav*/
.content-nav{
	border-top:1px solid #f07;
	background-color:#Fe94aa;	
}
.content-nav-wrap{
	position:relative;
	height:440px;
	margin:0px auto;
}
.content-nav img.spe{
	position:absolute;
	top:0px;
	left:257px;
	height:440px;
}
.content-nav-wrap .goods{
	float:left;
	overflow:hidden;
	width:257px;
	height:436px;
	padding-top:4px;
	background-color:#FFeaee;
}
.content-nav-wrap .goods li{
	display:block;
	height:31px;
	line-height:31px;
	padding-left:14px;
}
.content-nav-wrap .goods li a.spefont{
	font-size:15px;
	color:#000;
}
.content-nav-wrap .goods li img{
	width:12px;
	height:12px;
}
.content-nav-wrap .goods li .initial-c{
	color:#F07;
}
.content-nav-wrap .goods li a:not(.spefont){
	margin-left:16px;
}
.content-nav-wrap .goods li a:not(.spea):hover{
	text-decoration:underline;
}
.content-nav-wrap .goods li a.spea:hover > span{
	text-decoration:underline;
}
.content-nav-wrap .goods li:hover{
	background-color:#FFF;
}
.content-nav-wrap .goods li:hover{
	padding-left:12px;
}
.content-nav-wrap .goods li.color-f:hover{
	border-left:2px solid #FF5588;	
}
.content-nav-wrap .goods li.color-s:hover{
	border-left:2px solid #44BB77;
}
.content-nav-wrap .goods li.color-a:hover{
	border-left:2px solid #FF9922;
}
.content-nav-wrap .goods li.color-b:hover{
	border-left:2px solid #5577FF;
}
.content-nav-wrap .goods li.color-c:hover{
	border-left:2px solid #3399FF;
}
.content-nav-wrap .goods li.color-d:hover{
	border-left:2px solid #6644FF;
}
.content-nav-wrap .goods li.color-e:hover{
	border-left:2px solid #EE66EE;
}
.content-nav-wrap .goods li.color-live:hover{
	border-left:2px solid #FF5588;
}
.content-nav-wrap .goods li.color-birth:hover{
	border-left:2px solid #FF44BB;
}

.content-nav-wrap .goods div.section-size{
	position:absolute;
	top:0;
	left:257px;
	display:block;
	width:767px;
	height:438px;
	border:1px solid #eee;
	border-left:none;
	background-color:#FFF;
	
}
.content-nav-wrap .goods div.section-size a
{margin:0;
}
.content-nav-wrap .goods div.section-size a{
	display:inline-block;
}
.content-nav-wrap .goods div.section-size .leftsection{
	float:left;
	width:480px;
	height:420px;
	padding:20px 20px 0;
}
.content-nav-wrap .goods div.section-size .leftsection .sub{
	width:480px;
	margin-bottom:15px;
}
.content-nav-wrap .goods div.section-size .leftsection .sub a.font{
	float:left;
	text-decoration:none;
}
.content-nav-wrap .goods div.section-size .leftsection .sub a.font > span{
	
	color:#333;
	font-size:14px;
	font-weight:bold;
}
.content-nav-wrap .goods div.section-size .leftsection .sub a.c333{
	float:right;
	text-decoration:none;
}
.content-nav-wrap .goods div.section-size .leftsection .sub b{
	margin-left:5px;
	font-size:14px;
}

.content-nav-wrap .goods div.section-size .leftsection .goods-name{
	width:420px;
	height:48px;
	line-height:24px;
}
.content-nav-wrap .goods div.section-size .leftsection .goods-name a{
	display:inline-block;
	margin-right:16px;
	text-decoration-color:#f07;
}
.content-nav-wrap .goods div.section-size .leftsection .goods-name a:hover{
	/* color:#f07; */
}
.content-nav-wrap .goods div.section-size .leftsection .goods-name a:hover >span{
	color:#f07;
}
.content-nav-wrap .goods div.section-size .rightsection{
	float:left;
	width:216px;
	height:440px;
	padding:0 15px;
	border-left:1px solid #eee;
}
/* .rightsection start */
.content-nav-wrap .goods div.section-size .rightsection p{
	width:216px;
	height:50px;
	line-height:50px;
	text-align:center;
	font-size:20px;
	color:#333;
	font-weight:bold;
}
.content-nav-wrap .goods div.section-size .rightsection .f-l{
	float:left;
	/* width:100px; */
}
.content-nav-wrap .goods div.section-size .rightsection .f-r{
	float:right;
}
.content-nav-wrap .goods div.section-size .rightsection .f a{
	text-decoration:none;
}
.content-nav-wrap .goods div.section-size .rightsection .f div{
	display:block;
	width:100px;
	height:120px;
	margin-bottom:8px;
}
.content-nav-wrap .goods div.section-size .rightsection .f img{
	width:100px;
	height:100px;
	vertical-align:middle;
}
.content-nav-wrap .goods div.section-size .rightsection .f p{
	width:100px;
	height:20px;
	line-height:20px;
	font-size:12px;
	color:#666;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
/* userlogin */
.content-nav-wrap .userlogin{
	float:right;
	width:166px;
	height:420px;
}
.content-nav-wrap .userlogin .login-box{
	width:166px;
	height:195px;
	margin-top:10px;
	padding-top:10px;
	background-color:#FEEBF2;

}
.content-nav-wrap .userlogin a{
	display:block;
}
.content-nav-wrap .userlogin a.user-p{
	width:72px;
	height:72px;
	margin:0 auto;
	border-radius:50%;
	background:url('../images/content-image/nav-image/user.png') no-repeat center  #FF94C6;
	background-size:30px 30px;
}
.content-nav-wrap .userlogin p{
	width:166px;
	height:18px;
	margin:5px 0 7px 0;
	text-align:center;
}
.content-nav-wrap .userlogin p span.c333{
	color:#333;
}
.content-nav-wrap .userlogin p span.weight{
	font-weight:bold;
	width:90%;
	color:#333;
}
.content-nav-wrap .userlogin .vip-c{
	width:70px;
	height:25px;
	margin:10px auto 0;
	border:1px solid #f07;
	text-align:center;
	line-height:25px;
}
.content-nav-wrap .userlogin .vip-c span{
	color:#ff5777;
}
.content-nav-wrap .userlogin p.top-title{
	width:166px;
	height:25px;
	color:#333;
	font-size:16px;
	overflow:hidden;
}
.content-nav-wrap .userlogin p.bottom-title{
	color:#666;
	font-size:14px;
	overflow:hidden;
}
.content-nav-wrap .userlogin img{
	display:block;
	width:130px;
	height:130px;
	margin:10px auto 0;
}
.content-nav-wrap .userlogin .gain{
}